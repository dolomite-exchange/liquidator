{
  "name": "liquidator",
  "version": "1.0.0",
  "description": "Microservice to bid on Dolomite liquidation auctions",
  "main": "build/src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "coverage": "NODE_ENV=test jest --coverage --runInBand --forceExit --coverageReporters=text-lcov | node_modules/.bin/coveralls",
    "build": "rm -rf build/ && node_modules/.bin/tsc",
    "start": "node --max-old-space-size=4096 build/src/index.js",
    "start:ts": "ts-node src/index.ts",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "compose-clean": "docker-compose rm -f",
    "migrate": "echo 'Nothing to migrate'",
    "seed": "echo 'Nothing to seed'",
    "execute:get-active-users-csv": "STATS=true npx ts-node scripts/get-active-users-csv.ts",
    "execute:get-accrued-earnings": "STATS=true npx ts-node scripts/get-accrued-earnings.ts",
    "execute:get-daily-stats": "STATS=true npx ts-node scripts/get-daily-stats.ts",
    "execute:get-stats-per-frequency": "STATS=true npx ts-node scripts/get-stats-per-frequency.ts",
    "execute:get-lp-yield": "STATS=true npx ts-node scripts/get-lp-yield.ts",
    "execute:get-yt-glp-balances": "NETWORK_ID=42161 STATS=true npx ts-node scripts/get-yt-glp-balances.ts",
    "execute:get-total-active-users": "STATS=true npx ts-node scripts/get-total-active-users.ts",
    "execute:migrate-market": "npx ts-node scripts/migrate-market.ts",
    "execute:verify-bad-debt": "npx ts-node scripts/verify-bad-debt.ts",
    "execute:verify-glp-balances": "NETWORK_ID=42161 STATS=true npx ts-node scripts/verify-glp-balances.ts",
    "execute:verify-market-state": "npx ts-node scripts/verify-market-state.ts",
    "execute:verify-plv-glp-balances": "NETWORK_ID=42161 STATS=true npx ts-node scripts/verify-plv-glp-balances.ts",
    "execute:verify-yt-glp-balances": "NETWORK_ID=42161 STATS=true npx ts-node scripts/verify-yt-glp-balances.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolomite-exchange/liquidator.git"
  },
  "author": "Dolomite",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dolomite-exchange/liquidator/issues"
  },
  "homepage": "https://github.com/dolomite-exchange/liquidator#readme",
  "dependencies": {
    "@dolomite-exchange/dolomite-margin": "^0.14.2",
    "@dolomite-exchange/zap-sdk": "^0.4.3",
    "axios": "^0.29.0",
    "dotenv-flow": "^0.4.0",
    "ethers": "^5.5.3",
    "lru-cache": "^5.1.1",
    "luxon": "^1.28.1",
    "request-promise-native": "^1.0.9",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/luxon": "^1.26.5",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "coveralls": "^3.0.14",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "install": "^0.13.0",
    "jest": "^26.3.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^1.19.4",
    "npm": "^8.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
