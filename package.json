{
  "name": "liquidator",
  "version": "1.0.0",
  "description": "Microservice to bid on Dolomite liquidation auctions",
  "main": "build/src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "coverage": "NODE_ENV=test jest --coverage --runInBand --forceExit --coverageReporters=text-lcov | node_modules/.bin/coveralls",
    "build": "rm -rf build/ && node_modules/.bin/tsc",
    "start": "node build/src/index.js",
    "lint": "eslint . --ext .js --ignore-path .gitignore",
    "compose-clean": "docker-compose rm -f",
    "migrate": "echo 'Nothing to migrate'",
    "seed": "echo 'Nothing to seed'",
    "check-bad-debt": "ENV_FILENAME=liquidator.arbitrum-one.production.env npx ts-node ./src/get-bad-debt.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolomite-exchange/liquidator.git"
  },
  "author": "Dolomite",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dolomite-exchange/liquidator/issues"
  },
  "homepage": "https://github.com/dolomite-exchange/liquidator#readme",
  "dependencies": {
    "@dolomite-exchange/dolomite-margin": "^0.9.10",
    "axios": "^1.1.3",
    "dotenv-flow": "^0.4.0",
    "ethers": "^5.5.3",
    "lru-cache": "^5.1.1",
    "luxon": "^1.25.0",
    "request-promise-native": "^1.0.9",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "coveralls": "^3.0.14",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "install": "^0.13.0",
    "jest": "^26.3.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^1.19.4",
    "npm": "^8.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.7.4"
  }
}
